# Ignore project-specific files.
ExportedObj/
.consulo/
.vscode/
.idea/
*.csproj
*.unityproj
*.sln
*.suo
*.tmp
*.user
*.userprefs
*.pidb
*.booproj
*.svd
*.pdb
*.opendb
*.VC.db
*.pidb.meta
*.unitypackage

# Ignore crash reports.
sysinfo.txt

# Ignore build results.
build/
Build/
Builds/
builds/
*.apk
*.aab
*.xcodeproj
*.xcworkspace
*.xcworkspacedata
*.hmap
*.ipa
*.app
*.xcuserstate
*.xcuserdata
*.bundle

# Ignore asset meta data.
*.meta

# Ignore Unity3D generated file on crash reports.
sysinfo.txt

# Ignore gradle files.
gradle/
*.gradle
*.class
*.jar
*.war
*.nar
*.ear
*.class
*.rar
*.zip
*.tar.gz
*.gz
*.bz2
*.7z
*.xz

# Ignore crash reports and debug logs.
*.stackdump
*.stacktrace

# Ignore node_modules
node_modules/
npm-debug.log
yarn-error.log
yarn-debug.log

# Ignore Unity Cloud Build files
/UnityBuild/

